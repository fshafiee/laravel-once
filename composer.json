{
  "name": "fshafiee/laravel-once",
  "description": "Task rollup for Laravel",
  "version": "1.0.2",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/fshafiee/laravel-once",
  "keywords": [
    "events",
    "laravel",
    "once",
    "rollup"
  ],
  "authors": [
    {
      "name": "Farzad Shafiee",
      "email": "frzd.shafiee@gmail.com"
    }
  ],
  "support": {
    "email": "frzd.shafiee@gmail.com"
  },
  "scripts": {
    "cs": "vendor/bin/phpcs --standard=PSR2 src",
    "test": "vendor/bin/phpunit tests"
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^3.6.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.0.0",
    "composer/semver": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelOnce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelOnce\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelOnce\\Services\\OnceServiceProvider"
      ]
    }
  }
}
