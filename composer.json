{
  "name": "fshafiee/laravel-once",
  "description": "Task rollup for Laravel",
  "version": "1.0.0",
  "type": "library",
  "authors": [
    {
      "name": "Farzad Shafiee",
      "email": "frzd.shafiee@gmail.com"
    }
  ],
  "scripts": {
    "cs": "vendor/bin/phpcs --standard=PSR2 src",
    "test": "vendor/bin/phpunit tests"
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^3.6.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.0.0",
    "composer/semver": "^2.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Once\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laravel\\Once\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Once\\Services\\OnceServiceProvider"
      ]
    }
  }
}
